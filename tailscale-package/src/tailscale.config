config tailscale 'config'
	option enabled '0'
	option port '41641'
	option log_level ''
	option extra_args ''

config tailscale 'hardware'
	# Set by postinst based on detected hardware, can be overridden
	option mem_limit ''
	# GOMEMLIMIT value (e.g., '50MiB', '75MiB') - empty for no limit
	option gogc ''
	# GOGC value (e.g., '50') - empty for Go default (100)
	option no_logs '0'
	# Use --no-logs-no-support flag and disable stdout/stderr
	option has_wwan '0'
	# Watch wwan interfaces for reconnection triggers
